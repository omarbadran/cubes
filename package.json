{
  "name": "cubes",
  "packageManager": "yarn@3.2.2",
  "author": "Omar Badran",
  "workspaces": [
    "packages/sdk/*",
    "packages/apps/*",
    "packages/modules/*"
  ],
  "scripts": {
    "touch:package": "scripts:touch",
    "format": "prettier --check packages/**/*.{ts,tsx,js,json,md,yml} --write"
  },
  "devDependencies": {
    "@cubes/scripts": "workspace:^",
    "@types/node": "^18.7.9",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
